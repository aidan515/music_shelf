<header class="home-nav">
  <%= image_tag("square_logo.png", alt:"Square Logo", class: "logo")%>
  <div class="actions">
  	<% if current_user%>
  	Logged in as <%= current_user.email %>
  	<%= link_to "Log Out", logout_path%>
  	<% else %>
    <%= link_to "Sign Up", new_user_path %> or 
    <%= link_to "Log In", new_session_path%>
  </div>
 <% end %>
</header



						
